{
  "name": "@blocklet/observability",
  "version": "1.13.1",
  "description": "Blocklet Observability for tracing and observability",
  "publishConfig": {
    "access": "public"
  },
  "author": "Arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "homepage": "https://www.aigne.io/framework",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGNE-io/aigne-framework"
  },
  "bugs": {
    "url": "https://github.com/AIGNE-io/aigne-framework/issues"
  },
  "scripts": {
    "lint": "tsc --noEmit && biome check",
    "lint:fix": "biome check --write && pnpm run lint",
    "clean": "node scripts/build-clean.mjs",
    "dev": "blocklet dev",
    "start": "cross-env NODE_ENV=development nodemon --exec tsx api/dev.ts -w api",
    "bundle": "tsc --noEmit && npm run build:copy && npm run bundle:client && npm run bundle:api",
    "bundle:client": "vite build",
    "bundle:api": "npm run clean && tsc -p tsconfig.api.json && blocklet bundle --compact --create-release --external @aigne/sqlite",
    "build:copy": "zx --quiet scripts/download-json.mjs",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "bump-version": "zx --quiet scripts/bump-version.mjs"
  },
  "dependencies": {
    "@aigne/observability-api": "workspace:^",
    "@aigne/observability-ui": "workspace:^",
    "@aigne/sqlite": "workspace:^",
    "@arcblock/did-connect-react": "^3.1.28",
    "@arcblock/ux": "^3.1.28",
    "@biomejs/biome": "^2.1.2",
    "@biomejs/js-api": "^2.0.3",
    "@biomejs/wasm-nodejs": "^2.1.2",
    "@blocklet/sdk": "^1.16.48",
    "@blocklet/ui-react": "^3.1.28",
    "@mui/material": "^7.2.0",
    "bumpp": "^10.2.0",
    "dotenv-flow": "^4.1.0",
    "express": "^5.1.0",
    "express-sse": "^1.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.30.1",
    "ufo": "^1.6.1",
    "vite-plugin-blocklet": "^0.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/dotenv-flow": "^3.3.3",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4",
    "zx": "^8.6.2"
  }
}
